{
  "name": "uwuifier",
  "version": "1.1.0",
  "description": "This very lightweight package allows you to uwuify any sentence or word (excluding URL's) with many configurable parameters while giving you access to many vewy kawaii sentences and faces",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "files": [
    "/dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Schotsl/uwufier.git"
  },
  "author": "David Brands & Sjors van Holst",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Schotsl/uwufier/issues"
  },
  "homepage": "https://github.com/Schotsl/uwufier#readme",
  "keywords": [
    "uwu",
    "UwU",
    "UWU",
    "owo",
    "OwO",
    "OWO",
    "anime",
    "Anime",
    "kawaii",
    "Kawaii"
  ],
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "jest": "^25.5.4",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "ts-jest": "^25.5.1",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.2"
  },
  "scripts": {
    "test": "jest --config jestconfig.json",
    "lint": "tslint -p tsconfig.json",
    "serve": "nodemon",
    "build": "npm run lint && npm run format && npm test",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build"
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run build",
    "ext": "ts"
  }
}
