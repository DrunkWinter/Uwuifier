{
  "name": "uwufier",
  "version": "1.0.0",
  "description": "This package wiww uwufy any sentence ow wowd and expowts a few vewy handy functions if youw into uwufying",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Schotsl/uwufier.git"
  },
  "author": "David Brands & Sjors van Holst",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Schotsl/uwufier/issues"
  },
  "homepage": "https://github.com/Schotsl/uwufier#readme",
  "devDependencies": {
    "nodemon": "^2.0.3",
    "prettier": "^2.0.4",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "serve": "nodemon",
    "build": "tsc && node dist/index.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json"
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run lint & npm run format & npm run build",
    "ext": "ts"
  },
  "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
  "lint": "tslint -p tsconfig.json"
}
