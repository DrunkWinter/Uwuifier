{
  "name": "uwuifier",
  "version": "2.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "files": [
    "/dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Schotsl/uwuifier.git"
  },
  "author": "Sjors van Holst",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Schotsl/uwuifier/issues"
  },
  "homepage": "https://github.com/Schotsl/uwuifier#readme",
  "keywords": [
    "Uwu",
    "OwO",
    "Emoji",
    "Anime",
    "Meme",
    "Kawaii",
    "Emote",
    "Memes",
    "Nuzzles",
    "Uwuifier",
    "Weeaboo"
  ],
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "jest": "^25.5.4",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "ts-jest": "^25.5.1",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.2"
  },
  "scripts": {
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",

    "serve": "nodemon",
    "build": "npm run lint && npm run format && npm run test",

    "prepare": "npm run build"
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run build",
    "ext": "ts"
  }
