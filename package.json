  "devDependencies": {
    "@types/jest": "^26.0.23",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "rollup": "^2.48.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^26.5.6",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
