{
  "name": "uwuifier",
  "version": "4.0.0",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "unpkg": "./dist/index.js",
  "types": "./dist/types/index.d.js",
  "author": "Sjors van Holst",
  "license": "MIT",
  "homepage": "https://github.com/Schotsl/uwuifier#readme",
